{
    "name": "naykel/authit",
    "description": "Auth package with views for NayKel Laravel applications.",
    "keywords": [
        "Laravel",
        "Authit"
    ],
    "authors": [
        {
            "name": "Nathan Watts",
            "email": "nathan@naykel.com.au",
            "homepage": "https://naykel.com.au"
        }
    ],
    "homepage": "https://naykel.com.au",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "naykel/gotime": "^2.0",
        "spatie/laravel-honeypot": "^4.6",
        "spatie/laravel-permission": "^6.8"
    },
    "require-dev": {
        "laravel/pint": "^1.25",
        "pestphp/pest": "^4.3",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "Naykel\\Authit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naykel\\Authit\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naykel\\Authit\\AuthitServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}