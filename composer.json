{
    "name": "naykel/authit",
    "description": "Auth package with views for NayKel Laravel applications.",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "authors": [
        {
            "name": "Nathan Watts",
            "email": "nathan@naykel.com.au",
            "homepage": "https://naykel.com.au"
        }
    ],
    "homepage": "https://github.com/naykel76/authit.git",
    "keywords": ["Laravel", "Authit"],
    "require": {
        "php": "^8.2",
        "livewire/livewire": "^3.4",
        "spatie/laravel-honeypot": "^4.3",
        "spatie/laravel-permission": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Naykel\\Authit\\": "src/",
            "Naykel\\Authit\\Database\\Seeders\\": "src/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Naykel\\Authit\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naykel\\Authit\\AuthitServiceProvider"
            ]
        }
    }
}
